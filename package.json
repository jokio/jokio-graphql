{
	"name": "jokio-graphql",
	"version": "0.3.2",
	"description": "graphql middleware for jokio",
	"main": "dist/index.js",
	"types": "dist/index.d.js",
	"scripts": {
		"start": "ts-node src",
		"test": "ts-node src/_test",
		"dev": "nodemon",
		"build": "tsc",
		"deploy": "tsc && npm publish"
	},
	"dependencies": {
		"jokio": "0.2.1",
		"apollo-engine": "^0.8.5",
		"apollo-link-http": "^1.3.2",
		"apollo-server-express": "^1.3.2",
		"axios": "^0.17.1",
		"graphql-tools": "^2.18.0",
		"graphql-yoga": "1.2.0",
		"lodash": "^4.17.4"
	},
	"devDependencies": {
		"@types/jest": "22.1.0",
		"jest": "22.1.4",
		"nodemon": "1.14.11",
		"ts-jest": "22.0.1",
		"ts-node": "4.0.1",
		"typescript": "2.6.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"jok"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jokio/jokio-graphql.git"
	},
	"author": "Jok Entertainers",
	"license": "MIT",
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
